<?xml version="1.0" encoding="UTF-8"?>
<suite name="Suite" parallel="tests">
    <test name="bitrise">
        <listeners>
            <listener class-name="com.qe.listeners.TestListener" preserve-order="true" />
        </listeners>
        <parameter name="emulator" value="true" preserve-order="true" />
        <parameter name="platformName" value="Android" />
        <parameter name="androidBitrise" value="true" />
        <parameter name="udid" value="emulator-5554" />
        <parameter name="deviceName" value="emulator" />
        <parameter name="systemPort" value="10000" />
        <parameter name="chromeDriverPort" value="11000" />
        <packages>
            <package name="com.qe.tests.smoke.*"/>
        </packages>
    </test>
</suite>